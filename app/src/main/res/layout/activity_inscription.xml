<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:orientation="vertical"
	android:background="#fff">
   
    <EditText
		android:id="@+id/firstnameInscriptionEditText"
		android:hint="Prénom"
		android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ- "
		android:layout_height="wrap_content"
		android:layout_width="200dp"
		android:layout_gravity="center_horizontal" >
		<requestFocus></requestFocus>
	</EditText>

	<EditText
		android:id="@+id/lastnameInscriptionEditText"
		android:hint="Nom de famille"
		android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ- "
		android:layout_height="wrap_content"
		android:layout_width="200dp"
		android:layout_gravity="center_horizontal" >
	</EditText>

	<EditText
		android:id="@+id/cellphoneInscriptionEditText"
		android:hint="Téléphone"
		android:inputType="phone"
		android:layout_width="200dp"
		android:layout_height="wrap_content"
		android:layout_gravity="center_horizontal" />

    <EditText
		android:id="@+id/ibanInscriptionEditText"
		android:hint="IBAN"
		android:inputType="number"
		android:digits="0123456789"
		android:layout_height="wrap_content"
		android:layout_width="200dp"
		android:layout_gravity="center_horizontal" />

	<EditText
		android:id="@+id/bicInscriptionEditText"
		android:hint="BIC"
		android:inputType="number"
		android:digits="0123456789"
		android:layout_height="wrap_content"
		android:layout_width="200dp"
		android:layout_gravity="center_horizontal" />

	<EditText
		android:id="@+id/passwordInscriptionEditText"
		android:hint="Mot de passe"
		android:inputType="textPassword"
		android:layout_height="wrap_content"
		android:layout_width="200dp"
		android:layout_gravity="center_horizontal" />

	 <Button
		android:id="@+id/validateIncriptionButton"
		android:text="@string/valider"
		android:layout_height="wrap_content"
		android:layout_width="108dp"
		 android:layout_gravity="center_horizontal" />

</LinearLayout>
